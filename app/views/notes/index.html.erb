<h1>Listing notes</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Date</th>
    <th>Categorie</th>
    <th>Duree</th>
    <th>Difficulte</th>
    <th>Texte</th>
    <th>Titre</th>
    <th>Commentaire</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.id %></td>
    <td><%= note.date %></td>
    <td><%= note.categorie %></td>
    <td><%= note.duree %></td>
    <td><%= note.difficulte %></td>
    <td><%= note.texte %></td>
    <td><%= note.titre %></td>
    <td><%= note.commentaire %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
